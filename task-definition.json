{
    "executionRoleArn": "arn:aws:iam::507610966726:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "charterup-operators",
        "image": "dock.coachrail.com:5000/charterup-operators:latest",
        "cpu": 2,
        "memory": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 0
          }
        ],
        "mountPoints": [
          {
            "containerPath": "/static-api",
            "readOnly": true,
            "sourceVolume": "static-api"
          }
        ],
        "secrets": [
          {
            "name": "VUE_APP_API_URL",
            "valueFrom": "/coachrail/api/url"
          }
        ]
      }
    ],
    "volumes": [
      {
        "name": "static-api",
        "host": {
          "sourcePath": "/efsMounts/shared/assets/static-api"
        }
      }
    ],
    "family": "charterup-operators"
  }